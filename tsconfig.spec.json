/* To learn more about this file see: https://angular.io/config/tsconfig. */
{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "outDir": "./out-tsc/spec",
    "types": [
      "jasmine"
    ]
  },
  "files": [
    "src/test.ts",
    "src/polyfills.ts"
  ],
  "include": [
    "src/**/*.karma.spec.ts",
    "src/**/*.d.ts"
  ],
  "exclude": [
    "node_modules",
    "src/**/*.jest.spec.ts",
    "src/**/*.pact.spec.ts",
  ]
}
